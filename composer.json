{
    "name": "decodelabs/exemplar",
    "description": "Powerful XML tools",
    "type": "library",
    "keywords": ["markup", "xml"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",

        "decodelabs/coercion": "^0.2",
        "decodelabs/elementary": "^0.2",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/glitch-support": "^0.4",
        "decodelabs/collections": "^0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1",
        "phpstan/extension-installer": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "symplify/easy-coding-standard": "^11",

        "decodelabs/phpstan-decodelabs": "^0.6",
        "decodelabs/atlas": "^0.10"
    },
    "suggest": {
        "decodelabs/atlas": "Save generated XML to file"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Exemplar\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
