{
    "name": "decodelabs/exemplar",
    "description": "Powerful XML tools",
    "type": "library",
    "keywords": [ "markup", "xml" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",

        "decodelabs/coercion": "^0.2",
        "decodelabs/collections": "^0.8",
        "decodelabs/elementary": "^0.3",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/glitch-support": "^0.4"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6",
        "decodelabs/atlas": "^0.10|^0.11"
    },
    "suggest": {
        "decodelabs/atlas": "Save generated XML to file"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Exemplar\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    }
}
