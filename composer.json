{
    "name": "decodelabs/exemplar",
    "description": "Powerful XML tools",
    "type": "library",
    "keywords": [ "markup", "xml" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/coercion": "^0.3",
        "decodelabs/collections": "^0.10",
        "decodelabs/elementary": "^0.4.4",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/glitch-support": "^0.5"
    },
    "require-dev": {
        "decodelabs/atlas": "^0.12",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "suggest": {
        "decodelabs/atlas": "Save generated XML to file"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Exemplar\\": "src/Exemplar",
            "DecodeLabs\\PHPStan\\": "src/PHPStan"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Exemplar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        },
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
